   "source": [
    "n, k = map(int, input().split())\n",
    "data = []\n",
    "for _ in range(n):\n",
    "    data.append(int(input()))\n",
    "\n",
    "data.sort(reverse=True)\n",
    "\n",
    "result = 0\n",
    "for i in data:\n",
    "    if k >= i:\n",
    "        result += k // i\n",
    "        k %= i\n",
    "print(result)"
   ]
  }
