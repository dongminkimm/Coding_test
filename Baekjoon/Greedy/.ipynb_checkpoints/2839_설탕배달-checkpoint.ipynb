    "x = int(input())\n",
    "\n",
    "cnt = 0\n",
    "\n",
    "while x >= 0:\n",
    "    if x % 5 == 0:\n",
    "        cnt += x // 5\n",
    "        print(cnt)\n",
    "        break\n",
    "    x -= 3\n",
    "    cnt += 1\n",
    "else:\n",
    "    print(-1)"
